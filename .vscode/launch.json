{
  "configurations": [
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Launch test files with TestCafe",
      "program": "${workspaceRoot}/node_modules/testcafe/bin/testcafe.js",
      "args": ["chrome", "${relativeFile}", "--reporter json"],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}"
    }
  ]
}
